{
  "name": "@yc-css-ui/core",
  "version": "0.1.0",
  "description": "YC-CSS-UI MCP核心分析引擎",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "css-tree": "^2.3.1",
    "postcss": "^8.4.35",
    "postcss-value-parser": "^4.2.0",
    "specificity": "^1.0.0"
  },
  "devDependencies": {
    "@types/css-tree": "^2.3.4",
    "@types/node": "^20.11.20",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "css",
    "analysis",
    "postcss",
    "ast"
  ],
  "author": "YC-CSS-UI MCP Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yc-css-ui/yc-css-ui-mcp.git",
    "directory": "packages/core"
  }
}
